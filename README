>>> import web
>>> 
>>> db = web.database(dbn='postgres', user='postgres', pw='qwerty', db='gmys')
>>> a =db.query("SELECT codigo, descrip FROM pais")
>>> dir(a)
['__doc__', '__getitem__', '__init__', '__iter__', '__len__', '__module__', '__nonzero__', 'c', 'i', 'list']
